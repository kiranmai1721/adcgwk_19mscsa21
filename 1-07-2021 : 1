import java.util.*;
class pattern1
{
 public static void main(String args[])
 {
  int n;
  Scanner scn=new Scanner(System.in);
  System.out.println("enter n value");
  n=scn.nextInt();
  for(int i=1;i<=n;i++)
  {
   for(int j=n;j>=i;j--)
   {
    System.out.print("* ");
   }
   System.out.println();
  }
 }
}
..............................................
OUTPUT :
       enter n value
       5
       * * * * * 
       * * * *
       * * * 
       * *
       *
