class Solution
{
public:
int removeDuplicates(vector < int > & nums)
{
if(nums.size() == 0) return 0;
int i=0;
for(int j=1;j<nums.size();j++)
{
if(nums[i] != nums[j])
{
nums[++i] = nums[j];
}
}
return i+1;
}
};
................................
OUTPUT:
     Your input
     [1,1,2]
     Output
     [1,2]
     Expected
     [1,2]
